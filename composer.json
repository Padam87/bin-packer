{
    "name": "padam87/bin-packer",
    "description": "2D bin packing for PHP",
    "type": "library",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "ext-gd": "*",
        "ext-imagick": "*"
    },
    "suggest": {
        "ext-gd": "To use the visualizer.",
        "ext-imagick": "To use the GIF maker."
    },
    "autoload": {
        "psr-4": {
            "Padam87\\BinPacker\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padam87\\BinPacker\\Tests\\" : "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Prager",
            "email": "prager.adam87@gmail.com"
        }
    ]
}
